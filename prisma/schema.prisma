generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          String   @id @default(cuid())
  code        String   @unique
  longUrl     String
  clickCount  Int      @default(0)
  lastClicked DateTime?
  createdAt   DateTime @default(now())
}
